{
  "name": "SnailFM",
  "version": "2.1.8",
  "description": "SnailFM allows music with a simple click! Auto music, auto crediting, volume sliders and more. We provide the best nocopyright music made for your streams! If you are a streamer, please use StreamBop!",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/snaildos/SnailFM-Application.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "changelog": "gitmoji-changelog",
    "build": "electron-builder build --win --linux appimage --publish never",
    "snap": "electron-builder build --linux snap --publish never",
    "deploy": "electron-builder build --win --linux appimage --publish always"
  },
  "author": "SnailDOS",
  "license": "ISC",
  "devDependencies": {
    "electron": "25.8.1",
    "electron-builder": "24.4.0",
    "gitmoji-changelog": "2.3.0"
  },
  "build": {
    "protocols": [
      {
        "name": "snailfm",
        "schemes": [
          "snailfm"
        ]
      }
    ]
  },
  "dependencies": {
    "axios": "1.6.0",
    "bufferutil": "^4.0.3",
    "discord-rpc": "4.0.1",
    "electron-store": "8.1.0",
    "electron-titlebar-respect": "1.1.1",
    "electron-updater": "6.1.1",
    "glasstron-clarity": "0.1.26",
    "is-online": "9.0.1",
    "isdev": "1.0.1",
    "jquery": "3.7.0",
    "universal-analytics": "0.5.3",
    "utf-8-validate": "^6.0.0",
    "uuidv4": "6.2.13"
  }
}
